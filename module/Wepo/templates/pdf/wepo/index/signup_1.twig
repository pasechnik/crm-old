{% extends 'layout/layout.twig' %}
{% set title = translate('Sign up') %}
{% block title %}{{parent()}} - {{ title }}{% endblock title %}
{% block content %}
<h1>{{title}}</h1>
<div class="wform">
    {{form().openTag(form)|raw}}
    {{formCollection(form)}}
    {{form().closeTag(form)|raw}}
</div>
<div>{{message}}</div>
<a href="{{ url( 'index', { 'action': 'index' } ) }}">{{translate('Back to main page')}}</a>
{% endblock content %}
