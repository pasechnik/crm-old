{% extends 'partial/view.twig' %}
{% set title = 'Product: ' ~ model.product %}
{% block title %}{{parent()}} - {{ title }}{% endblock title %}
{% block photo %}
{% endblock photo %}
{% block actions %}
    <a href="{{ url( 'orderdetail', { 'action': 'edit', 'orderid':''~model.order_id, 'id':model.id() } )~saurl }}">{{translate('Edit')}}</a>
